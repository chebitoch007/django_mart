{
  "compilerOptions": {
    "target": "ES2017",
    "module": "None",
    "moduleResolution": "node",
    "lib": ["ES2017", "DOM", "DOM.Iterable"],
    "allowJs": true,
    "outDir": "../users/static/js",
    "rootDir": ".",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": false,
    "sourceMap": true,
    "removeComments": true,
    "noEmitOnError": false
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "build.ts",
    "../static/js"
  ]
}

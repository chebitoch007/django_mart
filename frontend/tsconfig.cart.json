{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["DOM", "ES2020"],
    "module": "commonjs",
    "outDir": "../cart/static/cart/js",
    "rootDir": "./src/cart",
    "sourceMap": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": [
    "src/cart/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}